<h1>
  <b><%=h @m33object.fileid.upcase %></b> <!--(<%=h @iclst+1 %> out of <%=h @m33brick.numberall %>)-->
</h1>

<% if flash[:message] %>
  <div><%= flash[:message] %></div>
<% end %>

<!-- Navigation -->
<p>
<% if not (@pm33object.nil?) %><%= link_to '<< Previous Object', cshow_m33object_path(@pm33object), :accesskey => "p" %><% end %>
<% if not (@nm33object.nil?) %><%= link_to 'Next Object >>', cshow_m33object_path(@nm33object), :accesskey => "n" %><% end %>
</p>
<p>
<%= link_to 'Back to Object Index', csort_m33brick_path(@m33brick) %>
</p>

<!-- Basic Data Table -->
<table>
  <tr>
    <th>RA</th>
    <th>Dec</th>
    <th>Subimage</th>
    <th>ViewFrac</th>
    <th>CFrac</th>
  </tr>
  <tr>
    <td><%=h @m33object.ra %></td>
    <td><%=h @m33object.dec %></td>
    <td><%=h @m33object.subimg %></td>
    <td <%=h @m33object.prioritycolor %> ><%=h @m33object.viewfrac %></td>
    <td <%=h @m33object.prioritycolor %> ><%=h @m33object.cfrac %></td>
  </tr>
</table>
<br />

<!-- Imaging -->
<table>
  <tr>
  	<th>Color</th>
<!--  	<th>336</th> -->
  	<th>475</th>
  	<th>814</th>
<!--  	<th>160</th> -->
  </tr>
  <tr>
  	<td><%= image_tag(@m33object.image_color, :height => '200') %></td>
<!--  	<td><%= image_tag(@m33object.image_336, :height => '200') %></td> -->
  	<td><%= image_tag(@m33object.image_475, :height => '200') %></td>
  	<td><%= image_tag(@m33object.image_814, :height => '200') %></td>
<!--  	<td><%= image_tag(@m33object.image_160, :height => '200') %></td> -->
  </tr>
</table>
<br />

<!-- CMD -->
<%= image_tag(@m33object.cmd_image, :height => '400') %>
<br />

<!-- Navigation -->
<p>
<% if not (@pm33object.nil?) %><%= link_to '<< Previous Object', cshow_m33object_path(@pm33object), :accesskey => "p" %><% end %>
<% if not (@nm33object.nil?) %><%= link_to 'Next Object >>', cshow_m33object_path(@nm33object), :accesskey => "n" %><% end %>
</p>
<p>
<%= link_to 'Back to Object Index', csort_m33brick_path(@m33brick) %>
</p>
